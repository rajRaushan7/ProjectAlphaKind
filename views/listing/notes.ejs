<%layout("/layouts/boilerplate") %>

    <body>
        <h1>All Notes List</h1>
        <div class="card-container">
            <% for (let notes of allNotes) { %>
                <div class="card">
                    <div class="notes-title">
                        <%= notes.title %>
                    </div>
                    <div class="notes-btn">
                        <a href="<%= notes.pdfURL %>" target="_blank">Open Notes</a><br>
                    </div>
                </div>
                <% } %>
        </div>

    </body>